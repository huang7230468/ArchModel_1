<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        <mapper namespace="ant.dubbo.dao.TradeDetailDao">
            <select id="queryById" resultType="TradeDetail" parameterType="long">
                SELECT tradeCode ,deatil
                FROM TradeDetail
                WHERE tradeCode = #{tradeDetailId}
             </select>
        </mapper>